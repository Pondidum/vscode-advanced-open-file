{
  "name": "vscode-advanced-file-open",
  "displayName": "vscode-advanced-open-file",
  "description": "Advanced File Open dialog",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.18.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.advancedOpenFile"
  ],
  "main": "./src/extensions",
  "contributes": {
    "commands": [
      {
        "command": "extension.advancedOpenFile",
        "title": "Advanced Open File"
      }
    ]
  },
  "dependencies": {
    "mkdirp": "^0.5.1",
    "touch": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.1",
    "jest": "^21.2.1",
    "regenerator-runtime": "^0.11.0",
    "vscode": "^1.1.6"
  },
  "scripts": {
    "build": "babel src --out-dir dist",
    "vscode:prepublish": "babel src --out-dir dist",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pondidum/vscode-afo.git"
  },
  "keywords": [
    "vscode",
    "advanced",
    "file",
    "open",
    "dialog"
  ],
